<UserControl x:Class="Capercali.WPF.UserControls.EventWindowCommands"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             
             >
    <StackPanel Orientation="Horizontal">
        <ComboBox ItemsSource="{Binding Ports}" SelectedValue="{Binding SelectedValue}"  SelectedValuePath="Key" DisplayMemberPath="Value" SelectionChanged="ComboBox_SelectionChanged"></ComboBox>
        <ComboBox></ComboBox>
    </StackPanel>

</UserControl>
